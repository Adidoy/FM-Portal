@model PUPFMIS.Models.OperatingExpenditures

<div class="modal-header">
    <h4 class="modal-title">View Operating Expenditure</h4>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>

<div class="modal-body" id="form-body">
    <table class="table">
        <tr>
            <td><strong>@Html.DisplayShortNameFor(model => model.UACS)</strong></td>
            <td>@Html.DisplayFor(model => model.UACS)</td>
        </tr>
        <tr>
            <td><strong>@Html.DisplayNameFor(model => model.ObjectName)</strong></td>
            <td>@Html.DisplayFor(model => model.ObjectName)</td>
        </tr>
    </table>
</div>

<div class="modal-footer">
    <div class="form-group">
        <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
    </div>
</div>