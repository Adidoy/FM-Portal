@model PUPFMIS.Models.MarketSurveyVM

@{
    ViewBag.Title = "Market Survey";
    ViewBag.Description = "Update";
    Layout = "~/Views/Shared/_BodyLayout.cshtml";
}

@section BreadCrumb {
    <ol class="breadcrumb float-sm-right">
        <li class="breadcrumb-item"><a href="/"><i class="fa fa-tachometer-alt"></i>&nbsp;@Html.ActionLink("PUP-FM Portal", "index", "Home", new { Area = "" }, null)</a></li>
        <li class="breadcrumb-item">@Html.ActionLink("Market Survey", "dashboard")</li>
        <li class="breadcrumb-item">Update</li>
    </ol>
}

@section Header {
    <h3 class="card-title"><i class="fa fa-list"></i> Update Market Survey</h3>
}

@using (Ajax.BeginForm("update", "MarketSurvey", new AjaxOptions() { HttpMethod = "POST", UpdateTargetId = "form-body", OnSuccess = "CreateResult" }, htmlAttributes: new { id = "marketSurvey", Area = "responsibility-centers" }))
{
    @Html.AntiForgeryToken()
    if (!Html.ViewData.ModelState.IsValid)
    {
        <div class="row">
            <div class="col-sm-12">
                <div class="alert alert-danger pl-2 pr-2 ml-2 mr-2 pb-0 pt-2">
                    <p class="p-0 m-0">
                        <h6><i class="icon fas fa-ban"></i> Alert!</h6>
                        @Html.ValidationSummary(true, "", new { style = "font-size: 10pt;" })
                    </p>
                </div>
            </div>
        </div>
        <br />
    }

    <div class="row">
        <div class="col-lg-4 col-md-4 col-sm-12">
            <div class="form-group-sm">
                <div style="text-align:center;" id="itemImageHolder">
                    @Html.HiddenFor(model => model.ItemImage)
                    @if (Model.ItemImage != null)
                    {
                        var base64 = Convert.ToBase64String(Model.ItemImage);
                        var imgsrc = string.Format("data:image/jpg;base64,{0}", base64);
                        <img src='@imgsrc' style="max-width:85%; max-height:85%; position:absolute; left:50%; top:50%; transform:translate(-50%, -50%);" />
                    }
                    else
                    {
                        <img src="@Url.Content("~/Content/imgs/PUPLogo.png")" style="max-width:85%; max-height:85%; position:absolute; left:50%; top:50%; transform:translate(-50%, -50%);" />
                    }
                </div>
            </div>
        </div>
        <div class="col-lg-8 col-md-8 col-sm-12">
            <div class="row">
                <div class="col-sm-12">
                    <div class="bg-info">
                        <h5 class="p-2">@Model.ItemName <small>(@Model.ItemCode)</small></h5>
                    </div>
                </div>
            </div>
            <h6>ITEM DETAILS:</h6>
            <div class="row">
                <div class="col-sm-3"><b>Item Type</b></div>
                @Html.HiddenFor(modelItem => modelItem.ItemType)
                <div class="col-sm-1">:</div>
                <div class="col-sm-8"><i>@Html.DisplayFor(modelItem => modelItem.ItemType)</i></div>
            </div>
            <div class="row">
                <div class="col-sm-3"><b>Category</b></div>
                @Html.HiddenFor(modelItem => modelItem.Category)
                <div class="col-sm-1">:</div>
                <div class="col-sm-8"><i>@Html.DisplayFor(modelItem => modelItem.Category)</i></div>
            </div>
            <div class="row">
                <div class="col-sm-3"><b>Procurement Source</b></div>
                <div class="col-sm-1">:</div>
                @Html.HiddenFor(modelItem => modelItem.ProcurementSource)
                <div class="col-sm-8"><i>@Html.DisplayFor(modelItem => modelItem.ProcurementSource)</i></div>
            </div>
            <div class="row">
                <div class="col-sm-3"><b>Unit of Measure</b></div>
                <div class="col-sm-1">:</div>
                @Html.HiddenFor(modelItem => modelItem.IndividualUOMReference)
                <div class="col-sm-8"><i>@Html.DisplayFor(modelItem => modelItem.IndividualUOMReference)</i></div>
            </div>
            <div class="row">
                <div class="col-sm-3"><b>Minimum Issuance Qty</b></div>
                <div class="col-sm-1">:</div>
                @Html.HiddenFor(modelItem => modelItem.MinimumIssuanceQty)
                <div class="col-sm-8"><i>@Html.DisplayFor(modelItem => modelItem.MinimumIssuanceQty)<span>&nbsp;</span>@Html.DisplayFor(modelItem => modelItem.IndividualUOMReference)</i></div>
            </div>
        </div>
    </div>
    <br /><br />
    <div class="row">
        <div class="col-sm-12">
            <div class="bg-info">
                <h5 class="p-2">ITEM SPECIFICATIONS</h5>
            </div>
            <br />
            <div class="ml-5 mr-5">
                <p style="text-align:justify; font-size:10pt;">@Html.DisplayFor(modelItem => modelItem.ItemSpecifications)</p>
            </div>
        </div>
    </div>
    <hr />
    <h4>Supplier Information</h4>
    <hr />
    <div class="row">
        <div class="col-sm-12">
            <div class="alert alert-info pl-2 pr-2 ml-2 mr-2 pb-0 pt-2">
                <p class="p-0 m-0">
                    <h6><i class="icon fas fa-exclamation-circle"></i> Notes:</h6>
                    <ul>
                        <li>
                            <i>Please provide, as much as possible, three (3) Supplier Information with their corresponding quouted price.</i>
                        </li>
                        <li>
                            <i>Provide all necessary documents such as quotations, market prices, etc. upon submission of PPMP.</i>
                        </li>
                        <li>
                            <i>If details for Supplier 2 and Supplier 3 are left blank, this means that the item requirement has a <b>single distributor</b> and necessary additional supporting documents must be submitted together with the PPMP.</i>
                        </li>
                    </ul>
                </p>
            </div>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-sm-12">
            <div class="card card-info card-outline">
                <div class="card-header">
                    <h3 class="card-title">SUPPLIER INFORMATION:</h3>
