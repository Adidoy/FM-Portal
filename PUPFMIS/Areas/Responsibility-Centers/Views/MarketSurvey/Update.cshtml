@section FormFooter{
    <div class="row">
        <div class="col-sm-12">
            <div style="text-align: right;">
                <button type="button" class="btn btn-md btn-primary" onclick="SaveMarketSurvey()"> <i class="fa fa-save"></i> Update Market Survey</button>
                <button type="button" class="btn btn-md btn-danger" onclick="window.location = '@Url.Action("items", "MarketSurvey", new { Area = "responsibility-centers" })'"> <i class="fa fa-arrow-circle-left"></i> Back to Item List</button>
            </div>
        </div>
    </div>
}

<div class="modal" id="modal-default">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content" id="modal-container">
        </div>
    </div>
</div>

<script type="text/javascript">
    var options = { "backdrop": "static", keyboard: true };
    function loadSupplier(url) {
        var supplier1 = $('#supplierID').val();
        var supplier2 = $('#supplier2ID').val();
        var supplier3 = $('#supplier3ID').val();
        url = url.replace("SupplierID1", supplier1 == "" ? 0 : supplier1);
        url = url.replace("SupplierID2", supplier2 == "" ? 0 : supplier2);
        url = url.replace("SupplierID3", supplier3 == "" ? 0 : supplier3);
        $.ajax({
            type: "GET",
            url: url,
            contentType: "application/json; charset=utf-8",
            datatype: "json",
            success: function (data) {
                $('#modal-container').html(data);
                $('#modal-default').modal(options);
                $('#modal-default').modal('show');
            }
        });
    };
    function clearSupplierData(supplierNo) {
        if (supplierNo == 1) {
            $('#supplierID').val("");
            $('#supplierName').val("");
            $('#supplierAddress').val("");
            $('#supplierContact').val("");
            $('#supplierEmail').val("");
            $('#Supplier1UnitCost').val("0.00");
        }
        if (supplierNo == 2) {
            $('#supplier2ID').val("");
            $('#supplier2Name').val("");
            $('#supplier2Address').val("");
            $('#supplier2Contact').val("");
            $('#supplier2Email').val("");
            $('#Supplier2UnitCost').val("0.00");
        }
        if (supplierNo == 3) {
            $('#supplier3ID').val("");
            $('#supplier3Name').val("");
            $('#supplier3Address').val("");
            $('#supplier3Contact').val("");
            $('#supplier3Email').val("");
            $('#Supplier3UnitCost').val("0.00");
        }
    };
    function SaveMarketSurvey() {
        swal.fire({
            title: "Save Market SUrvey?",
            text: "You are about to update this Market Survey, continue?",
            icon: "question",
            showCancelButton: true,
        }).then((result) => {
            if (result.value) {
                $("#marketSurvey").submit();
            }
            else {
                swal.fire({
                    title: "Cancelled!",
                    text: "You have cancelled saving the record.",
                    icon: "error"
                })
            };
        })
    }
    function CreateResult(data) {
        if (data.result == true) {
            swal.fire({
                title: "Market Survey Saved!",
                text: "Market Survey is successfully saved.",
                icon: "success"
            }).then((result) => {
                if (result.value) {
                    var url = "@Url.Action("items", "MarketSUrvey", new { Area = "responsibility-centers" })";
                    window.location.href = url;
                }
            })
        }
        else {
            swal.fire({
                title: "Market Survey Not Saved!",
                text: "Saving this Market Survey failed.",
                icon: "error"
            });
        }
    }
</script>